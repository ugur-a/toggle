public class Main {
    public static void main(String[] args) {
       String [] ogrenciler = new String[3];
       ogrenciler [0] = "UÄŸur";
       ogrenciler [1] = "Sevda";
       ogrenciler [2] = "Su";
       for (int i=0; i<ogrenciler.length; i++){
           System.out.println(ogrenciler[i]);
       }
       System.out.println("------------------------------");
       for (String ogrenci : ogrenciler){
           System.out.println(ogrenci);
       }
    }
}
